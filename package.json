{
  "name": "essex-powerbi-template",
  "version": "1.0.0",
  "description": "PowerBI custom visual.",
  "main": "src/index.js",
  "scripts": {
    "preinstall:prereq": "npm install tar.gz mv",
    "preinstall:submodules": "node bin/downloadPrivateSubmodules",
    "preinstall:dependencies": "node bin/installSubmoduleDeps",
    "preinstall": "npm run clean && npm run preinstall:prereq && npm run preinstall:submodules && npm run preinstall:dependencies",
    "install-certificate": "node bin/openCert",
    "start": "npm run dev",
    "tdd": "npm-run-all --parallel dev test:tdd",
    "dev": "mkdirp $npm_package_config_tmpDrop/assets && npm-run-all --parallel dev:style dev:pbiviz dev:serve dev:watch:*",
    "dev:serve": "http-server $npm_package_config_tmpDrop --silent --cors --port 8080 --ssl -c-1 --cert certs/PowerBICustomVisualTest_public.crt --key certs/PowerBICustomVisualTest_private.key",
    "dev:pbiviz": "node bin/addCapabilitiesToPbivizJson > $npm_package_config_tmpDrop/assets/pbiviz.json",
    "dev:status": "date +%s > $npm_package_config_tmpDrop/assets/status",
    "dev:style": "node-sass $(json -f pbiviz.json style) > $npm_package_config_tmpDrop/assets/visual.css",
    "dev:watch:pbiresource": "onchange $npm_package_config_tmpDrop/**/*.js $npm_package_config_tmpDrop/**/*.json $npm_package_config_tmpDrop/**/*.css -- npm run dev:status",
    "dev:watch:script": "webpack --watch --color --output-filename $npm_package_config_tmpDrop/assets/visual.js | sed '/node_modules/ { N; N; d; }'",
    "dev:watch:style": "onchange 'style/**/*' -- npm run dev:style",
    "dev:watch:pbiconfig": "onchange 'pbiviz.json' $(json -f pbiviz.json capabilities) -- npm run dev:pbiviz",
    "package": "mkdirp dist && npm run package:code && node bin/packageVisual $npm_package_config_tmpProd",
    "package:code": "mkdirp $npm_package_config_tmpProd && npm-run-all --parallel package:script package:style",
    "package:script": "webpack --color --output-filename $npm_package_config_tmpProd/visual.js | sed '/node_modules/ { N; N; d; }'",
    "package:style": "node-sass $(json -f pbiviz.json style) > $npm_package_config_tmpProd/visual.css",
    "test": "karma start --color | sed '/node_modules/ { N; N; d; }'",
    "test:tdd": "karma start --color --tdd | sed '/node_modules/ { N; N; d; }'",
    "clean": "rm -rf node_modules .tmp"
  },
  "config": {
    "tmpDrop": "./.tmp/drop",
    "tmpProd": "./.tmp/prod"
  },
  "keywords": [],
  "author": "Uncharted Software Inc.",
  "license": "MIT",
  "privateSubmodules": {
    "@uncharted/personas": "0.4.22",
    "uncharted-facets": "2.3.0",
    "@uncharted/stories.common": "0.1.7",
    "@uncharted/strippets": "0.4.6",
    "@uncharted/thumbnails": "0.3.6"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.33",
    "@types/mocha": "^2.2.32",
    "@types/sinon": "^1.16.31",
    "@types/sinon-chai": "^2.7.27",
    "@types/requirejs": "^2.1.28",
    "chai": "^3.5.0",
    "http-server": "^0.9.0",
    "json": "^9.0.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.0",
    "node-zip": "^1.1.1",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.2",
    "powerbi-visuals": "^0.5.9",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "jquery": "^3.1.1"
  }
}
